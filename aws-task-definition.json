{
  "containerDefinitions": [ 
     {
        "entryPoint": [
           "sh",
           "-c"
        ],
        "essential": true,
        "image": "node:18-alpine",
        "name": "web-app",
        "portMappings": [ 
           { 
              "containerPort": 80,
              "hostPort": 80,
              "protocol": "tcp"
           }
        ]
     }
  ],
  "cpu": "256",
  "family": "job_analysis",
  "memory": "512",
  "networkMode": "awsvpc",
  "runtimePlatform": {
       "operatingSystemFamily": "LINUX"
   },
  "requiresCompatibilities": [ 
      "FARGATE" 
   ]
}